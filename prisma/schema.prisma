generator client {
  provider = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["public", "vgtpbx_tenant"]
}

model auth_group {
  id                                Int                                 @id @default(autoincrement())
  name                              String                              @unique @db.VarChar(150)
  auth_group_permissions            auth_group_permissions[]
  auth_user_groups                  auth_user_groups[]
  pbx_contacts_groups               pbx_contacts_groups[]
  pbx_device_vendor_function_groups pbx_device_vendor_function_groups[]
  pbx_menu_item_groups              pbx_menu_item_groups[]

  @@index([name], map: "auth_group_name_idx")

  @@schema("public")
}

model auth_permission {
  id                         Int                          @id @default(autoincrement())
  name                       String                       @db.VarChar(255)
  codename                   String                       @db.VarChar(100)
  auth_group_permissions     auth_group_permissions[]
  auth_user_user_permissions auth_user_user_permissions[]

  @@unique([codename], map: "auth_permission_codename_unique")

  @@schema("public")
}

model auth_group_permissions {
  id              BigInt          @id @default(autoincrement())
  groupId         Int            
  permissionId    Int             
  auth_permission auth_permission @relation(fields: [permissionId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  auth_group      auth_group      @relation(fields: [groupId], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@unique([groupId, permissionId])
  @@index([groupId])
  @@index([permissionId])

  @@schema("public")
}

model auth_tenant {
  id          String        @id @db.VarChar(50)
  accountId   String        @db.VarChar(50) @unique
  createdAt   DateTime      @default(now())
  updatedAt   DateTime      @updatedAt
  name        String        @db.VarChar(100)
  domain      String        @db.VarChar(100)
  description String?       @db.VarChar(500)
  logo        String?       @db.VarChar(255)
  plan        String        @default("basic") @db.VarChar(20)
  maxUsers    Int           @default(5)
  disabled    Boolean       @default(false)
  users       auth_user[]
  pbxDomains  pbx_domains[]
  messages             Message[]
  webhooks             Webhook[]
  integrations         Integration[]
  notificationSettings NotificationSettings?
  userMappings         auth_user_mapping[]
  domainMappings       domain_mapping[]

  @@schema("public")

}

model auth_user {
  uid                        String                       @id @unique @db.VarChar(50)
  updatedAt                  DateTime                     @updatedAt
  email                      String                       @unique @db.VarChar(255)
  displayName                String?                      @db.VarChar(100)
  firstName                  String?                      @db.VarChar(150)
  lastName                   String?                      @db.VarChar(150)
  avatar                     String?                      @db.VarChar(255)
  phoneNumber                String?                      @db.VarChar(20)
  isSuperuser               Boolean                       @default(false)
  isAdmin                   Boolean                       @default(false)
  isStaff                   Boolean                       @default(false)
  emailVerified             Boolean                       @default(false)
  disabled                  Boolean                       @default(false)
  tenantId                  String                        @db.VarChar(50)
  createdAt                 DateTime?
  lastSignInAt              DateTime?                     
  auth_tenant               auth_tenant                  @relation(fields: [tenantId], references: [id])
  auth_user_groups          auth_user_groups[]
  auth_user_user_permissions  auth_user_user_permissions[]
  authtoken_token                 authtoken_token?
  pbx_users                  pbx_users?
  mapping                   auth_user_mapping?

  @@index([tenantId])
  @@schema("public")
}

model auth_user_groups {
  id         BigInt     @id @default(autoincrement())
  uid        String     
  groupId    Int        
  auth_group auth_group @relation(fields: [groupId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  auth_user  auth_user  @relation(fields: [uid], references: [uid], onDelete: NoAction, onUpdate: NoAction)

  @@unique([uid, groupId])
  @@index([groupId])
  @@index([uid])

  @@schema("public")
}

model auth_user_user_permissions {
  id              BigInt          @id @default(autoincrement())
  uid             String          
  permissionId    Int
  auth_permission auth_permission @relation(fields: [permissionId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  auth_user       auth_user       @relation(fields: [uid], references: [uid], onDelete: NoAction, onUpdate: NoAction)

  @@unique([uid, permissionId])
  @@index([permissionId])
  @@index([uid])

  @@schema("public")
}

model authtoken_token {
  key       String    @id @db.VarChar(40)
  created   DateTime  @db.Timestamptz(6)
  uid       String    @unique @db.VarChar(50)
  auth_user auth_user @relation(fields: [uid], references: [uid], onDelete: NoAction, onUpdate: NoAction)

  @@index([key])

  @@schema("public")
}

model auth_user_mapping {
  uid         String   @id @unique @db.VarChar(50)
  accountId   String   @db.VarChar(50)
  schemaName  String   @db.VarChar(100)
  tenantId    String   @db.VarChar(50)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  user        auth_user  @relation(fields: [uid], references: [uid], onDelete: Cascade)
  tenant      auth_tenant @relation(fields: [tenantId], references: [id], onDelete: Cascade)

  @@index([accountId])
  @@index([tenantId])
  @@schema("public")
}

model domain_mapping {
  id          String   @id @default(cuid()) @db.VarChar(50)
  fullDomain  String   @unique @db.VarChar(255)
  fullDomainUid    String   @db.VarChar(50)
  tenantId    String   @db.VarChar(50)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  tenant      auth_tenant @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  @@index([tenantId])
  @@schema("public")
}


